{
    "name": "arif/laravel-unique-slug",
    "description": "A simple unique slug generator for the Laravel applications",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arif\\LaravelSlugGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ariful Islam Arif",
            "email": "devboyarif@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arif\\LaravelUniqueSlug\\UniqueSlugServiceProvider"
            ],
            "aliases": {
                "UniqueSlug": "Arif\\LaravelUniqueSlug\\UniqueSlug"
            }
        }
    }
}
